<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <Version>1.0.0</Version>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of Bessie.</Description>
        <Copyright>Copyright (c) 2023 Samuel Lucas</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/Bessie.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/Bessie.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>bessie aead blake3</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Blake3" Version="0.6.0" />
    </ItemGroup>

</Project>
